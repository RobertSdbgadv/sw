-- Scripts:

local function THNUIH_fake_script() -- TextLabel.Script 
	local script = Instance.new('Script', TextLabel)

	while task.wait() do
		script.Parent.Text = "Position : " ..tostring(math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.X))..", "..tostring(math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y))..", "..tostring(math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Z))
	end
	
end
coroutine.wrap(THNUIH_fake_script)()
local function RDSHID_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	function Clicked() 
		setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position))
		script.Parent.Text = "Copied to clipboard!"
		task.wait(3)
		script.Parent.Text = "Generate Tween scirpt"
	end
	
	script.Parent.MouseButton1Down:connect(Clicked)
end
coroutine.wrap(RDSHID_fake_script)()
local function GBCRQL_fake_script() -- TextButton_2.Script 
	local script = Instance.new('Script', TextButton_2)

	function Clicked() 
		local e = tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
		setclipboard('local f = 0.5 -- Time\n\n\n\ntweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(f, Enum.EasingStyle.Linear)\ntweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new('..e..')}):Play()')
		script.Parent.Text = "Copied to clipboard!"
		task.wait(3)
		script.Parent.Text = "Generate Tween scirpt"
	end
	
	script.Parent.MouseButton1Down:connect(Clicked)
end
coroutine.wrap(GBCRQL_fake_script)()
local function KTTZK_fake_script() -- TextButton_3.Script 
	local script = Instance.new('Script', TextButton_3)

	function Clicked() 
		e=tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
		setclipboard("game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new("..e..")")
		script.Parent.Text = "Copied to clipboard!"
		task.wait(3)
		script.Parent.Text = "Generate TP Script"
	
	end
	script.Parent.MouseButton1Down:connect(Clicked)
end
coroutine.wrap(KTTZK_fake_script)()
local function OVOVCFQ_fake_script() -- TextLabel_9.Script 
	local script = Instance.new('Script', TextLabel_9)

	local text = script.Parent
	local add = 10
	wait(1)
	local k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(k/255,0/255,0/255)
		k = k + add
		wait()
	end
	while true do
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,k/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,255/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255,k/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255 - k/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(k/255,0/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,0/255,255/255 - k/255)
			k = k + add
			wait()
		end
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,0/255,0/255)
			k = k + add
			wait()
		end
	end
end
coroutine.wrap(OVOVCFQ_fake_script)()
local function MRGSD_fake_script() -- TextButton_4.Script 
	local script = Instance.new('Script', TextButton_4)

	function Clicked() 
		script.Parent.Parent.Parent.Parent.ScreenGui:Destroy()
	end
	
	script.Parent.MouseButton1Down:connect(Clicked)
end
coroutine.wrap(MRGSD_fake_script)()
local function RRWDSZS_fake_script() -- CoordsFinderV3.Script 
	local script = Instance.new('Script', CoordsFinderV3)

	frame = script.Parent.Frame
	frame.Draggable = true
	frame.Selectable = true
	frame.Active = true
end
coroutine.wrap(RRWDSZS_fake_script)()
